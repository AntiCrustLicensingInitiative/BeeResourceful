{
  "schemaVersion": 1,
  "id": "beeresourceful",
  "version": "${version}",
  "name": "Bee Resourceful",
  "description": "Adds nectars for Bee Productive",
  "authors": [
    "Martmists"
  ],
  "contact": {
    "sources": "https://dilaton.martmists.com/martmists/beeresourceful"
  },
  "license": "MIT",
  "icon": "assets/beeresourceful/icon.png",
  "entrypoints": {
    "main": [
      {
        "value": "com.martmists.beeresourceful.BeeResourceful",
        "adapter": "kotlin"
      }
    ]
  },
  "custom": {
    "waila:plugins": {
      "id": "beeresourceful:bpdata_plugin",
      "initializer": "com.martmists.beeresourceful.waila.BPDataPlugin"
    }
  },
  "depends": {
    "fabricloader": ">=0.4.0",
    "fabric": "*",
    "beeproductive": "*",
    "libcd": "*",
    "fabric-language-kotlin": "*"
  },
  "suggests": {
    "mobz": "*",
    "oysters": "*"
  }
}
